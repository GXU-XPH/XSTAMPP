<plugin>
   <extension
         point="xstampp.extension.steppedProcess">
      <process
            DataModelClass="xstampp.stpapriv.model.PrivacyController"
            command="stpasec.command.load"
            editorId="stpasec.steps.step1_1"
            extension="sec"
            extensionDescriptions="extended STPA Project;STPA Project"
            icon="icons/Logo_small_sec.png"
            id="stpasec.navigation.process"
            name="STPA for Security Project"
            newWizard="stpasec.wizards.NewWizard"
            runCommand="xstampp.stpasec.command.run">
         <category
               icon="icons/branding/step1EstablishFundamentals.png"
               id="stpasec.navigation.step1"
               name="Establish Fundamentals">
            <step
                  editorId="stpasec.steps.step1_1"
                  icon="icons/buttons/navigation/STEP1_1(16).png"
                  id="stpasec.navigation.step1_1"
                  name="System Description">
            </step>
            <step
                  editorId="stpasec.steps.step1_6"
                  icon="icons/buttons/navigation/STEP1_6(16).png"
                  id="stpasec.navigation.step1_6"
                  name="System Goals">
            </step>
            <step
                  editorId="stpasec.steps.step1_2"
                  icon="icons/buttons/navigation/STEP1_2(16).png"
                  id="stpasec.navigation.step1_2"
                  name="Losses">
            </step>
            <step
                  editorId="stpasec.steps.step1_3"
                  icon="icons/buttons/navigation/STEP1_3(16).png"
                  id="stpasec.navigation.step1_3"
                  name="Vulnerabilities">
            </step>
            <step
                  editorId="stpasec.steps.step1_4"
                  icon="icons/buttons/navigation/STEP1_4(16).png"
                  id="stpasec.navigation.step1_4"
                  isVisible="&quot;bla&quot;"
                  name="Linking of Losses and Vulnerabilities">
            </step>
            <step
                  editorId="stpasec.steps.step1_5"
                  icon="icons/buttons/navigation/STEP1_5(16).png"
                  id="stpasec.navigation.step1_5"
                  name="Security Constraints">
            </step>
            <step
                  editorId="stpasec.steps.step1_7"
                  icon="icons/buttons/navigation/STEP1_7(16).png"
                  id="stpasec.navigation.step1_7"
                  name="Design Requirements">
            </step>
            <editor_List
                  command="xstampp.astpa.newControlStructure.command"
                  editorId="astpa.steps.step1_8"
                  icon="icons/buttons/navigation/STEP1_8(16).png"
                  id="xstampp.stpasec.controlStructure_List"
                  isVisible="astpa.use.multics"
                  name="Control Structure Diagrams"
                  provider="xstampp.astpa.controlstructure.ControlStructureProvider">
               <commandAddition
                     commandId="xstampp.astpa.command.cs.rename"
                     name="Rename">
               </commandAddition>
               <commandAddition
                     commandId="xstampp.astpa.command.cs.deleteRoot">
               </commandAddition>
            </editor_List>
            <step
                  default_editor="true"
                  editorId="stpasec.steps.step1_8"
                  icon="icons/buttons/navigation/STEP1_8(16).png"
                  id="xstampp.stpasec.step1_8"
                  isVisible="astpa.use.singlecs"
                  name="High Level Control Structure Model">
            </step>
         </category>
         <category
               icon="icons/branding/step2ControlActions.png"
               id="stpasec.category2"
               name="Unsecure Control Actions">
            <step
                  editorId="stpasec.steps.step2_1"
                  icon="icons/buttons/navigation/STEP2_1(16).png"
                  id="stpasec.navigation.step2_1"
                  name="Control Actions">
            </step>
            <step
                  editorId="stpasec.steps.step2_2"
                  icon="icons/buttons/navigation/STEP2_2(16).png"
                  id="stpasec.navigation.step2_2"
                  name="Unsecure Control Actions Table">
            </step>
            <step
                  editorId="stpasec.steps.step2_3"
                  icon="icons/buttons/navigation/STEP2_3(16).png"
                  id="stpasec.navigation.step2_3"
                  name="Corresponding Security Constraints">
            </step>
         </category>
         <category
               icon="icons/branding/step3CausalAnalysis.png"
               id="stpasec.category3"
               name="Causal Analysis">
            <step
                  editorId="stpasec.steps.step3_1"
                  icon="icons/buttons/navigation/STEP3_1(16).png"
                  id="stpasec.navigation.step3_1"
                  name="Control Structure With Process Model">
            </step>
            <step
                  editorId="stpasec.steps.step3_2"
                  icon="icons/buttons/navigation/STEP3_2(16).png"
                  id="stpasec.navigation.step3_2"
                  name="Causal Factors Table">
            </step>
            <step
                  default_editor="true"
                  editorId="stpasec.editor.results"
                  id="stpasec.navigation.step3_3"
                  name="STPA-Sec Results">
            </step>
         </category>
      </process>
      <SettingsPage
            class="xstampp.astpa.preferences.ProjectSpecifics"
            id="xstampp.astpa.settings.general"
            name="General">
      </SettingsPage>
      <SettingsPage
            class="xstampp.astpa.usermanagement.settings.ResponsibilitySettings"
            id="xstampp.astpa.settings.responsibilities"
            name="Responibilities">
      </SettingsPage>
   </extension>
