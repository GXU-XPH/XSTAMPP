<extension
      point="org.eclipse.ui.bindings">
   <scheme
         id="bindingscheme"
         name="bindingscheme">
   </scheme>
   <key
         commandId="xstampp.command.load"
         schemeId="bindingscheme"
         sequence="M1+O">
   </key>
   <key
         commandId="astpa.exit"
         schemeId="bindingscheme"
         sequence="M1+W">
   </key>
   <key
         commandId="astpa.save"
         schemeId="bindingscheme"
         sequence="M1+S">
   </key>
   <key
         commandId="astpa.saveas"
         schemeId="bindingscheme"
         sequence="M1+M2+S">
   </key>
   <key
         commandId="astpa.exit"
         schemeId="bindingscheme"
         sequence="M1+Q">
   </key>
   <key
         commandId="org.eclipse.ui.file.export"
         schemeId="bindingscheme"
         sequence="M1+E">
   </key>
   <key
         commandId="astpa.commands.expandAll"
         contextId="xstampp.navigation.context"
         schemeId="bindingscheme"
         sequence="ARROW_LEFT">
      <parameter
            id="xstampp.expand"
            value="False">
      </parameter>
   </key>
   <key
         commandId="astpa.commands.expandAll"
         contextId="xstampp.navigation.context"
         schemeId="bindingscheme"
         sequence="ARROW_RIGHT">
      <parameter
            id="xstampp.expand"
            value="True">
      </parameter>
   </key>
   <key
         commandId="astpa.command.openStep"
         contextId="xstampp.navigation.context"
         schemeId="bindingscheme"
         sequence="CR">
   </key>
   <key
         commandId="org.eclipse.ui.window.nextView"
         contextId="org.eclipse.ui.contexts.window"
         schemeId="bindingscheme"
         sequence="M1+F7">
   </key>
   <key
         commandId="org.eclipse.ui.window.nextEditor"
         contextId="org.eclipse.ui.contexts.window"
         schemeId="bindingscheme"
         sequence="M1+F6">
   </key>
   <key
         commandId="org.eclipse.ui.window.previousEditor"
         contextId="org.eclipse.ui.contexts.window"
         schemeId="bindingscheme"
         sequence="M1+Shift+F6">
   </key>
   <key
         commandId="org.eclipse.ui.window.previousView"
         contextId="org.eclipse.ui.contexts.window"
         schemeId="bindingscheme"
         sequence="M1+SHIFT+F7">
   </key>
   <key
         commandId="org.eclipse.ui.window.switchToEditor"
         contextId="org.eclipse.ui.contexts.window"
         schemeId="bindingscheme"
         sequence="M1+SHIFT+E">
   </key>
   <key
         commandId="xstampp.ui.commands.selectAll"
         contextId="org.eclipse.ui.contexts.window"
         schemeId="bindingscheme"
         sequence="M1+A">
   </key>
   <key
         commandId="xstampp.command.undo"
         schemeId="bindingscheme"
         sequence="M1+Z">
   </key>
   <key
         commandId="xstampp.command.redo"
         schemeId="bindingscheme"
         sequence="M1+Y">
   </key>
</extension>
