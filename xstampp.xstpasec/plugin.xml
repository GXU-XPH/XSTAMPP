<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            class="xstpasec.ui.View"
            id="xstpasec.view.contextTables"
            name="%view.name.contextTables"
            restorable="true">
      </view>
   </extension>
   <extension
         point="xstampp.extension.stepEditors">
      <stepEditor
            editorId="xstpa.editor.context"
            icon="icons/logo.png"
            id="xstpasec.stepEditor1"
            name="%stepEditor.name.contextTables"
            parentStep="stpasec.navigation.step3_1">
      </stepEditor>
      <stepEditor
            editorId="xstpasec.editor.ruca"
            icon="icons/ruca_logo2x16.png"
            id="xstpasec.views.ruca"
            name="%stepEditor.name.RefinedUnsafeControlActions"
            parentStep="stpasec.navigation.step3_1">
      </stepEditor>
      <stepEditor
            editorId="xstpasec.editor.refinedSC"
            icon="icons/rsc_logox16.png"
            id="xstpasec.stepEditor.RefinedSC"
            name="%stepEditor.name.RefinedSafetyConstraints"
            parentStep="stpasec.navigation.step3_1">
      </stepEditor>
      <stepEditor
            editorId="xstpa.editor.basicScenarios"
            id="xstpasec.stepEditor.basicScenarios"
            name="Basic Scenarios"
            parentStep="stpasec.navigation.step3_1">
      </stepEditor>
      <stepEditor
            editorId="xstpasec.editor.ltl"
            id="xstpasec.stepEditor.ltl"
            name="LTL Tables"
            parentStep="stpasec.category3">
      </stepEditor>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="xstpasec.settings.PreferencePageSettings"
            id="xstpasec.preferences.choosePath"
            name="%page.name.XSTPA_Preferences">
      </page>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="xstpasec.ui.RefinedUCAView"
            default="false"
            icon="icons/ruca_logo2x16.png"
            id="xstpasec.editor.ruca"
            name="%editor.name.Refined_Unsafe_Control_Actions">
      </editor>
      <editor
            class="xstpasec.ui.RefinedSafetyConstraintsView"
            default="false"
            icon="icons/rsc_logox16.png"
            id="xstpasec.editor.refinedSC"
            name="%editor.name.Refined_Safety_Constraints">
      </editor>
      <editor
            class="xstpasec.ui.ltl.LTLPropertiesTable"
            default="false"
            id="xstpasec.editor.ltl"
            name="LTL Table">
      </editor>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="xstpasec.settings.PreferenceInitializer">
      </initializer>
   </extension>

</plugin>
